/* components/searchbar/searchbar.wxss */
.searchbar{
  background-color: #42BD55;
  padding: 20rpx;
}
.search-navigator, .search-inpuut-group{
  width: 100%;
  height: 70rpx;
  background-color: #fff;
  border-radius: 10rpx;
}
.search-navigator{
  background-image: url('data:image/svg+xml;base64,');
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 6%;
}
.search-input{
  min-height: 70rpx;
  height: 70rpx;
  font-size: 28rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}