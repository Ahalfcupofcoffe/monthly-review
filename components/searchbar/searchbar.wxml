<!--components/searchbar/searchbar.wxml-->
<view class='searchbar'>
  <navigator wx:if='{{isnavigator}}' url='/pages/search/search' class='search-navigator'></navigator>
  <view wx:else class='search-inpuut-group'>
    <input
     class='search-input' 
     placeholder='搜索' 
     focus='true'
     bindinput='inputEvent'
     >
    </input>
  </view>
</view>
