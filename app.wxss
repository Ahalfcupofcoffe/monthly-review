/**app.wxss**/
/* 字体图标 */
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-xingxingkong:before {
  content: "\e663";
}
.icon-attend:before {
  content: "\e674";
}
.icon-yuandian:before {
  content: "\e7c2";
}
.icon-zan:before {
  content: "\e647";
}

/* 清除scroll-view中导航条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}




